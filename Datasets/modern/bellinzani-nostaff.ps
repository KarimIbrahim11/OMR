/tabmrest { % usage x y n tabmrest - rest with n flags (modern style)
  gsave 1 setlinewidth
  3 1 roll moveto
  dup -4 le { % longa rest
    gsave 0.8 setlinewidth
    currentpoint -5 5 rmoveto
    10 0 rlineto 0 3 rlineto -10 0 rlineto 0 -3 rlineto fill
    moveto currentpoint -5 4 rmoveto 0 5 rlineto stroke
    moveto 5 0 rmoveto 0 9 rlineto stroke grestore
  } if
  dup -3 eq { % brevis rest
    gsave 0.8 setlinewidth
    currentpoint -5 1 rmoveto
    10 0 rlineto 0 3 rlineto -10 0 rlineto 0 -3 rlineto fill
    moveto currentpoint -5 0 rmoveto 0 5 rlineto stroke
    moveto 5 0 rmoveto 0 5 rlineto stroke grestore
  } if
  dup -2 eq { % whole note rest
    -5 3 rmoveto 10 0 rlineto
    currentpoint stroke moveto -2 -3 rmoveto
    0 3 rlineto -6 0 rlineto 0 -3 rlineto 6 0 rlineto fill
  } if
  dup -1 eq { % half note rest
    -5 0 rmoveto 10 0 rlineto
    currentpoint stroke moveto -2 0 rmoveto
    0 3 rlineto -6 0 rlineto 0 -3 rlineto 6 0 rlineto fill
} if
  dup 0 eq { % crotchet rest
    -2 4 1 4 3 3 rcurveto currentpoint -1.5 1.5 rlineto
    -2 1 -5 1 -3 -3 rcurveto 1.5 -1.5 rlineto fill moveto
    -1 1 rmoveto
    -5 5 3 4 -2 9 rcurveto -1.5 1.5 rlineto
    5 -5 -3 -4 2 -9 rcurveto 1.5 -1.5 rlineto fill
  } if
  dup 1 ge { % (semi)quaver rests
    2 8 rlineto dup dup 0.5 mul exch 2 mul rlineto
    currentpoint stroke moveto
    {
      -2 -2 -3 -2 -5 -2 rcurveto 
      currentpoint 1.5 add 1.5 -90 0 arcn 
      0 -1 2.5 0 3.5 0.5 rcurveto
      currentpoint fill moveto
      -1 -4 rmoveto
    } repeat
  } {pop} ifelse grestore
} bind def

