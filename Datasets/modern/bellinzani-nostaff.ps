/tabsflag { % x y n tabsflag - stem with n flags (simple style)
  gsave 1 setlinewidth
  3 1 roll moveto 0 20 rlineto
  currentpoint stroke moveto
  dup 0 lt {
    dup neg {
      -4 -2 rlineto 1 -1 rlineto 3 2 rlineto 0 1 rlineto
      currentpoint fill moveto 0 -5 rmoveto
    } repeat
  } if
  dup 0 gt {
    {
      1.2 -1 rlineto 
      2 -1 6 -1 6 -3 rcurveto
      -1.2 -1.2 rlineto
      0 2 -4 2 -6 3 rcurveto
      0 2.2 rlineto
      currentpoint fill moveto
      0 -5 rmoveto
    } repeat
  } {pop} ifelse  grestore
} bind def

/tabdflag { % x y n tabdflag - stem with n flags (diamond style)
  gsave 1 setlinewidth
  3 1 roll moveto
  dup 0 lt {
    4 4 rlineto currentpoint
    -0.4 0.4 rlineto -4 -4 rlineto 0.4 -0.4 rlineto fill moveto
    -4 4 rlineto currentpoint
    -1.1 -1.1 rlineto 4 -4 rlineto 1.1 1.1 rlineto fill moveto
    -4 -4 rlineto currentpoint
    0.4 -0.4 rlineto 4 4 rlineto -0.4 0.4 rlineto fill moveto
    4 -4 rlineto currentpoint
    1.1 1.1 rlineto -4 4 rlineto -1.1 -1.1 rlineto fill moveto
    0 8 rmoveto
  } {
    4 4 rlineto -4 4 rlineto -4 -4 rlineto 4 -4 rlineto
    currentpoint fill moveto 0 8 rmoveto
  } ifelse
  dup -2 gt {
    0 setlinejoin 0 setlinecap
    0 20 8 sub rlineto 
  } if
  dup 0 gt {
    {
      6 -3 rlineto
      -6 0 rmoveto
    } repeat
  } {pop} ifelse
  stroke grestore
} bind def

